Host *
	controlpath ~/.ssh/cm_socket/%r@%h:%p
	controlmaster auto
	controlpersist 3600
	AddKeysToAgent yes

Host chic-tunnel
	hostname chic-gw.es.net
	ForwardAgent yes
	dynamicforward localhost:3345
	user soehlert

Host lbl-tunnel
	hostname lbl-gw.es.net
	ForwardAgent yes
	dynamicforward localhost:12345
	user soehlert

Host personal.github.com
	HostName github.com
	User soehlert
	PreferredAuthentications publickey
	IdentityFile ~/.ssh/github
	IdentitiesOnly yes
