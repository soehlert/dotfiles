Host *
    controlpath ~/.ssh/cm_socket/%r@%h:%p
    controlmaster auto
    controlpersist 3600
    ForwardAgent yes

Host chic-tunnel
    hostname chic-gw.es.net
    dynamicforward localhost:12345
    user soehlert

Host lbl-tunnel
    hostname lbl-gw.es.net
    dynamicforward localhost:12345
    user soehlert

Host home-tunnel
    hostname home.samoehlert.com
    dynamicforward localhost:22345
    user soehlert

Host home
    hostname home.samoehlert.com
    user soehlert

Host bitbucket.org
    IdentityFile ~/.ssh/bitbucket

Host personal.github.com
    HostName github.com
    User soehlert
    PreferredAuthentications publickey
    IdentityFile ~/.ssh/github
    IdentitiesOnly yes
