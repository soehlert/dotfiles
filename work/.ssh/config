HashKnownHosts no
TCPKeepAlive no
ServerAliveCountMax 15
ServerAliveInterval 60
Compression no
UpdateHostKeys yes

CanonicalizeHostname Yes
CanonicalDomains es.net testbed100.es.net ocean.cu-es.net

ControlMaster auto
ControlPersist yes
ControlPath ~/.ssh/master-%r@%h:%p

Host *-gw.es.net
	ProxyCommand none

Host *.es.net !*-gw.es.net
	ProxyJump chic-gw
	ForwardAgent yes

Host bro-tb
	Hostname bro-tb.testbed100.es.net
	ForwardAgent yes
	ProxyCommand none

Host scan-test
	Hostname scan-test.ocean.cu-es.net
	ForwardAgent yes
	ProxyCommand none
	User root

Host panix
	Hostname vc136-90.vc.panix.com
	ProxyCommand none

Host personal.github.com
	HostName github.com
	User soehlert
	PreferredAuthentications publickey
