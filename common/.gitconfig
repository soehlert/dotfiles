[core]
  editor       = vim
  excludesfile = /Users/sam/.gitignore_global
  autocrlf     = false
  safecrlf     = true
[alias]
  a      = add
  br     = branch
  c      = commit
  cm	 = commit -m
  co     = "!f() { git checkout \"$@\" && git stash list | grep \"$1:\"; }; f"
  com    = !git checkout master
  cob    = checkout -b
  dp     = !git diff -R playground
  dm     = !git diff -R master
  po	 = "!git push -u origin $(git rev-parse --abbrev-ref HEAD)"
  st     = status
  up     = !git pull --rebase --prune $@ && git submodule update --init --recursive
  bclean = "!f() { git branch --merged ${1-master} | grep -v " ${1-master}$" | xargs git branch -d; }; f"
[push]
  default = simple
[credential]
	helper = none
[includeIf "gitdir:/Users/soehlert/projects/work/"]
    [user]
        name = Sam Oehlert
        email = soehlert@es.net
[includeIf "gitdir:/Users/soehlert/projects/personal/"]
    [user]
        name = Sam Oehlert
        email = sam.oehlert@gmail.com
