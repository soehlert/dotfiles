Host *
    controlpath ~/.ssh/cm_socket/%r@%h:%p
    controlmaster auto
    controlpersist 3600

Host tunnel
    hostname bastion-sec.ncsa.illinois.edu
    dynamicforward localhost:12345
    user soehlert

Host git-sec*
    identityfile ~/.ssh/git-sec
    hostname git-sec.internal.ncsa.edu
    proxycommand ssh tunnel -W %h:%p

Host server
    identityfile /Users/soehlert/.ssh/server
    hostname slammy.noip.me
    user sam
    port 22

