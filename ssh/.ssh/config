Host *
    controlpath ~/.ssh/cm_socket/%r@%h:%p
    controlmaster auto
    controlpersist 3600
    ForwardAgent yes

Host chic-tunnel
    hostname chic-gw.es.net
    dynamicforward localhost:12345
    user soehlert

Host lbl-tunnel
    hostname lbl-gw.es.net
    dynamicforward localhost:12345
    user soehlert

Host server
    identityfile /Users/soehlert/.ssh/server
    hostname slammy.noip.me
    user sam
    port 22

Host mani
    identityfile /Users/soehlert/.ssh/mini
    hostname 192.168.1.9
    user soehlert
    port 22

Host bitbucket.org
    IdentityFile ~/.ssh/bitbucket
