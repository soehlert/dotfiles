Host *
    controlpath ~/.ssh/cm_socket/%r@%h:%p
    controlmaster auto
    controlpersist 3600

Host tunnel
    hostname fog.ncsa.illinois.edu
    dynamicforward localhost:12345
    user soehlert

Host git-sec*
    identityfile ~/.ssh/git-sec
    hostname git-sec.internal.ncsa.edu
    proxycommand ssh tunnel -W %h:%p


Host github-blog
    identityfile /home/soehlert/.ssh/id_rsa.blog
    gssapidelegatecredentials no
    hostname github.com
    user soehlert
    forwardx11 yes
    gssapiauthentication no

Host server
    identityfile /Users/soehlert/.ssh/server
    hostname slammy.noip.me
    user sam
    port 22

