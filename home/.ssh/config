Host *
    controlpath ~/.ssh/cm_socket/%r@%h:%p
    controlmaster auto
    controlpersist 3600
    ForwardAgent yes

Host home-tunnel
    hostname slammy.noip.me
    dynamicforward localhost:22345
    user soehlert

Host home
    hostname home.samoehlert.com
    user soehlert

Host bitbucket.org
    IdentityFile ~/.ssh/bitbucket
