Host *
    controlpath ~/.ssh/cm_socket/%r@%h:%p
    controlmaster auto
    controlpersist 3600
    ForwardAgent yes
    AddKeysToAgent yes

Host home-tunnel
    hostname home.samoehlert.com
    dynamicforward localhost:22345
    user soehlert

Host home
    hostname home.samoehlert.com
    user soehlert

Host bitbucket.org
    IdentityFile ~/.ssh/bitbucket
